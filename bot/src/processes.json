{
  "apps": [
    {
      "name": "bedrock-bot",
      "args": [],
      "script": "node index.js",
      "exec_interpreter": "none",
      "autorestart": false,
      "watch": ["package.json"],
      "ignore_watch": [".git"],
      "env": {
        "tokens": {},
        "log_tokens": {
          "LOG_LEVEL": "info",
          "LOG_FILE_SIZE": "10m",
          "LOG_MAX_FILES": "5"
        }
      },
      "out_file": "log.output",
      "error_file": "err.output"
    }
  ]
}
